    "template": [
      "default",
      "_templates/dark"
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright &copy; 2021 Sana Commerce</span>",
      "_appTitle": "Sana Developer Portal",
      "_enableNewTab": true,
      "_enableSearch": true,
      "_disableContribution": false,
      "_appLogoPath": "logo.png",
      "_enableModelDebug": false,
      "_googleTagManagerId": "GTM-PW3S4D4",
      "_googleAnalyticsId": "UA-189400950-1"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "noLangKeyword": false
