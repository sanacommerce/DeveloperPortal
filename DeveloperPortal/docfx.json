{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          //"ADK/**.md",
          //"ADK/**/toc.yml",
          //"Graph-APIs/**.md",
          //"Graph-APIs/**/toc.yml",
          "**toc.yml",
          "**.md"
        ],
        "exclude": [
          "bin/**",
          "obj/**",
          "wwwroot/**",
          "_templates",
          "Properties",
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "robots.txt",
          "**/img/**"
        ]
      }
    ],
    "overwrite": [],
